{
  "manifest_version": 3,
  "name": "Simple Page Logger - v6",
  "version": "6.0",
  "description": "Logs every page visit",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Page Logger"
  }
}